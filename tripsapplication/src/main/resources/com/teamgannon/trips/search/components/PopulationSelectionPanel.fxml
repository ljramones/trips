<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10">
    <children>
        <Label fx:id="populationLabel" text="Population (magnitude)" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <CheckBox fx:id="yesNoPop" text="Yes?" GridPane.columnIndex="1" GridPane.rowIndex="0" />

        <CheckBox fx:id="pop0" text="1s" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <CheckBox fx:id="pop1" text="10s" GridPane.columnIndex="3" GridPane.rowIndex="0" />
        <CheckBox fx:id="pop2" text="100s" GridPane.columnIndex="4" GridPane.rowIndex="0" />
        <CheckBox fx:id="pop3" text="1000s" GridPane.columnIndex="5" GridPane.rowIndex="0" />
        <CheckBox fx:id="pop4" text="10⁴" GridPane.columnIndex="6" GridPane.rowIndex="0" />

        <CheckBox fx:id="pop5" text="10⁵" GridPane.columnIndex="2" GridPane.rowIndex="1" />
        <CheckBox fx:id="pop6" text="10⁶" GridPane.columnIndex="3" GridPane.rowIndex="1" />
        <CheckBox fx:id="pop7" text="10⁷" GridPane.columnIndex="4" GridPane.rowIndex="1" />
        <CheckBox fx:id="pop8" text="10⁸" GridPane.columnIndex="5" GridPane.rowIndex="1" />
        <CheckBox fx:id="pop9" text="10⁹" GridPane.columnIndex="6" GridPane.rowIndex="1" />
    </children>
</fx:root>
