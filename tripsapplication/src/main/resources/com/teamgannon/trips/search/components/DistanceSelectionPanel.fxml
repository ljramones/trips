<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import org.controlsfx.control.RangeSlider?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10">
    <children>
        <Label fx:id="distanceToEarthLabel" text="Radius from Center&#10;   (in ly)" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <TextField fx:id="lowRangeTextField" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <RangeSlider fx:id="d2EarthSlider" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <TextField fx:id="highRangeTextField" GridPane.columnIndex="3" GridPane.rowIndex="0" />
    </children>
</fx:root>
