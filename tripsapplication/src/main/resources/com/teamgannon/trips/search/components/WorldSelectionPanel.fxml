<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10">
    <children>
        <Label fx:id="worldLabel" text="World Present" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <CheckBox fx:id="yesNoWorld" text="Yes?" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <CheckBox fx:id="greenWorld" text="Green" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <CheckBox fx:id="greyWorld" text="Grey" GridPane.columnIndex="3" GridPane.rowIndex="0" />
        <CheckBox fx:id="brownWorld" text="Brown" GridPane.columnIndex="4" GridPane.rowIndex="0" />
    </children>
</fx:root>
