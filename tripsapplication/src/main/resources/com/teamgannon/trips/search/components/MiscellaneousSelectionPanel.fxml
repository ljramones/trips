<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10">
    <children>
        <Label fx:id="otherLabel" text="Other" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <CheckBox fx:id="otherPresent" text="Present?" GridPane.columnIndex="1" GridPane.rowIndex="0" />

        <Label fx:id="anomalyLabel" text="Anomaly" GridPane.columnIndex="0" GridPane.rowIndex="1" />
        <CheckBox fx:id="anomalyPresent" text="Present?" GridPane.columnIndex="1" GridPane.rowIndex="1" />
    </children>
</fx:root>
