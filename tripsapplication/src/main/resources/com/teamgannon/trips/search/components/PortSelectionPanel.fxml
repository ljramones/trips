<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.GridPane?>

<fx:root type="javafx.scene.layout.GridPane" xmlns:fx="http://javafx.com/fxml/1" hgap="10" vgap="10">
    <children>
        <Label fx:id="portLabel" text="Port Present" GridPane.columnIndex="0" GridPane.rowIndex="0" />
        <CheckBox fx:id="yesNoPort" text="Yes?" GridPane.columnIndex="1" GridPane.rowIndex="0" />
        <CheckBox fx:id="aPort" text="A" GridPane.columnIndex="2" GridPane.rowIndex="0" />
        <CheckBox fx:id="bPort" text="B" GridPane.columnIndex="3" GridPane.rowIndex="0" />
        <CheckBox fx:id="cPort" text="C" GridPane.columnIndex="4" GridPane.rowIndex="0" />
        <CheckBox fx:id="dPort" text="D" GridPane.columnIndex="5" GridPane.rowIndex="0" />
        <CheckBox fx:id="ePort" text="E" GridPane.columnIndex="6" GridPane.rowIndex="0" />
    </children>
</fx:root>
